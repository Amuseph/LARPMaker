<table class="table table-striped table-bordered table-sm mt-5">
  <thead class = 'thead-dark'>
  <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Start Date</th>
      <th>End Date</th>
  </tr>
  </thead>
  <tbody>
  <% @character.events.order('startdate desc').each do |event| %>
      <tr>
        <td><%= event.name %></td>
        <td><%= event.description %></td>
        <td><%= event.startdate %></td>
        <td><%= event.enddate %></td>
      </tr>
    <% end %>
    </tbody>
  </table>