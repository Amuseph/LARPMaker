<div class="modal-body">
  <%= select_tag 'skillgroups', options_from_collection_for_select(Skillgroup.all, :name, :name) %>
  <%= bootstrap_form_with(model: @characterskill, url: pathurl) do |f| %>
    <%= f.select :name, ['Adjustment', 'Donation', 'Event', 'Feedback Letter', 'Referral', 'Season Pass', 'Go Big Bonus', 'Setup/Take Down', 'Volunteer' ], label: 'Type' %>
    <%= f.text_field :description %>
    <%= f.text_field :aquiredate, label: 'Date: (YYYY-MM-DD)'%>
    <%= f.text_field :amount %>
    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
    <button type="submit" class="btn btn-primary">Submit</button>
  <% end %>
</div>