<div class="modal-body">
  <%= bootstrap_form_with(model: @eventattendance, url: admin_event_eventattendances_path) do |f| %>
    <%= f.collection_select :email, User.all, :email, :email %>
    <hr>
    <%= f.grouped_collection_select :character_id, User.order(:email), :characters, :email, :id, :name %>
    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
    <button type="submit" class="btn btn-primary">Submit</button>
  <% end %>
</div>    