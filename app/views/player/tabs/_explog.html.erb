<table border = 1>
<tr>
    <th>Name</th>
    <th>Description</th>
    <th>Aquire Date</th>
    <th>Amount</th>
    <th>Granted By</th>
</tr>
<% current_user.explogs.each do |explog| %>
    <tr>
      <td><%= explog.name %></td>
      <td><%= explog.description %></td>
      <td><%= explog.aquiredate.to_date %></td>
      <td><%= explog.amount %></td>
      <td><%= explog.grantedby.firstname + ' ' + explog.grantedby.lastname%> </td>
    </tr>
  <% end %>
  </table>